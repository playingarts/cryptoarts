{
  "name": "crypto-arts",
  "version": "1.0.1",
  "main": "source/",
  "license": "MIT",
  "description": "Playing Arts",
  "repository": "git@github.com:playingarts/cryptoarts.git",
  "author": "Gleb Ryshkov",
  "private": true,
  "proxy": "localhost:3000",
  "scripts": {
    "dev": "next dev",
    "dump": "tsx --env-file=.env ./dump",
    "build": "next build && tsc -P tsconfig.json --outDir dist --noEmit false --module commonjs",
    "start": "node dist/source",
    "lint": "yarn lint:tsc && yarn lint:next",
    "lint:tsc": "tsc -p tsconfig.json",
    "lint:next": "next lint",
    "lint:eslint": "eslint . --ext .ts,.tsx --ignore-pattern pages --ignore-pattern components",
    "schema": "ts-node --transpile-only -e \"import './source/graphql/generate'\" -O '{\"module\":\"commonjs\"}'",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest --watch",
    "testManual": "ts-node --transpile-only -r dotenv/config -O '{\"module\":\"commonjs\"}' ./__tests__/index",
    "testOpensea": "ts-node --transpile-only -r dotenv/config -O '{\"module\":\"commonjs\"}' ./__tests__/test",
    "buildIconsStories": "ts-node --transpile-only -r dotenv/config -O '{\"module\":\"commonjs\"}' ./components/Icons/a.iconsscript.ts",
    "gitDeleteBranches": "git branch | grep -vE \"master|prod\" | xargs git branch -D"
  },
  "engines": {
    "node": "^23.4"
  },
  "dependencies": {
    "@apollo/client": "^3.12.3",
    "@apollo/react-ssr": "^4.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/serialize": "^1.3.3",
    "@graphql-tools/stitch": "9.4.10",
    "@metamask/eth-sig-util": "^8.1.1",
    "apollo-link-schema": "^1.2.5",
    "chalk": "^5.3.0",
    "colord": "^2.9.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-graphql": "^0.12.0",
    "express-winston": "^4.2.0",
    "fp-ts": "^2.16.9",
    "graphql": "^15.8.0",
    "graphql-type-json": "^0.3.2",
    "graphql-type-long": "^0.1.1",
    "io-ts": "^2.2.22",
    "is-mobile": "^5.0.0",
    "just-intersect": "^4.3.0",
    "just-throttle": "^4.2.0",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.17",
    "metamask-react": "^2.7.0",
    "modern-normalize": "^3.0.1",
    "mongoose": "^8.9.0",
    "msw-storybook-addon": "^2.0.4",
    "next": "^15.2.2-canary.2",
    "next-routes": "^1.4.2",
    "nextjs-google-analytics": "^2.3.7",
    "react": "^19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.1",
    "react-hookstore": "^1.5.2",
    "react-resize-detector": "^12.0.2",
    "react-swipeable": "^7.0.2",
    "redirect-https": "^1.3.1",
    "shadertoy-react": "^1.1.2",
    "smoothscroll-polyfill": "^0.4.4",
    "store": "^2.0.12",
    "web3": "^4.16.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@emotion/babel-preset-css-prop": "^11.12.0",
    "@graphql-codegen/core": "^4.0.2",
    "@graphql-codegen/typescript": "^4.1.2",
    "@graphql-codegen/typescript-resolvers": "^4.4.1",
    "@next/bundle-analyzer": "^15.1.0",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-designs": "^8.2.0",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/nextjs": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@svgr/webpack": "^8.1.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/memoizee": "^0.4.11",
    "@types/node": "^22.10.2",
    "@types/react": "19.0.1",
    "@types/react-dom": "^19.0.3",
    "@types/smoothscroll-polyfill": "^0.3.4",
    "@types/store": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "babel-loader": "^9.2.1",
    "eslint": "9.17.0",
    "eslint-config-next": "15.1.0",
    "eslint-plugin-storybook": "^0.11.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.6.8",
    "node-mocks-http": "^1.16.2",
    "nodemon": "^3.1.9",
    "storybook": "^8.4.7",
    "storybook-addon-emotion-theme": "^2.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "5.7.2",
    "undici": "^7.1.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
