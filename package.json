{
  "name": "crypto-arts",
  "version": "1.0.1",
  "main": "source/",
  "license": "MIT",
  "description": "Playing Arts",
  "repository": "git@github.com:playingarts/cryptoarts.git",
  "author": "Gleb Ryshkov",
  "private": true,
  "proxy": "localhost:3000",
  "scripts": {
    "dev": "next dev",
    "dump": "tsx --env-file=.env ./dump",
    "build": "next build",
    "start": "next start",
    "lint": "yarn lint:tsc && yarn lint:next",
    "lint:tsc": "tsc -p tsconfig.json",
    "lint:next": "eslint .",
    "lint:eslint": "eslint . --ext .ts,.tsx --ignore-pattern pages --ignore-pattern components",
    "schema": "ts-node --transpile-only -e \"import './source/graphql/generate'\" -O '{\"module\":\"commonjs\"}'",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest --watch",
    "test:smoke": "jest --testPathPattern=smoke --runInBand --no-cache",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "testManual": "ts-node --transpile-only -r dotenv/config -O '{\"module\":\"commonjs\"}' ./__tests__/index",
    "testOpensea": "ts-node --transpile-only -r dotenv/config -O '{\"module\":\"commonjs\"}' ./__tests__/test",
    "buildIconsStories": "ts-node --transpile-only -r dotenv/config -O '{\"module\":\"commonjs\"}' ./components/Icons/a.iconsscript.ts",
    "gitDeleteBranches": "git branch | grep -vE \"master|prod\" | xargs git branch -D"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@apollo/client": "^4.0.0",
